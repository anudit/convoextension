{
  "name": "The Convo Space",
  "description": "The Convo Space",
  "manifest_version": 2,
  "version": "0.2.0",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "icon-128.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "webRequest",
    "tabs",
    "*://*/**",
    "storage"
  ]
}
